{
  "name": "Discord-Tool",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.4.2",
    "cross-env": "^7.0.2",
    "discord-oauth2": "^2.3.0",
    "discord.js": "11.6.4",
    "discord.js-light": "^3.5.9",
    "discord.js-self": "^12.0.4",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.2.0",
    "electron-open-link-in-browser": "^1.0.2",
    "package": "^1.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "electron-start": "electron .",
    "electron-build": "electron-builder",
    "release": "cross-env KEEP_CLASS_NAMES='true' yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package": "electron-packager ./build Discord-Tool --platform=mas --out=out "
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^18.3.7",
    "electron-builder": "^22.7.0",
    "electron-packager": "^15.0.0",
    "wait-on": "^5.1.0"
  }
}
